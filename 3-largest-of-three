public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	Integer result = 0;
    if (num1< num2){
        if(num2< num3){
            result = num3;
        } else {
        result = num2;
        }
    else if (num1 < num3) {
        result = num3;
    }
    else{
    result = num1;
    }
}
return result;
}