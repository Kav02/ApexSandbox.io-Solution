public Integer fibonacci(Integer n) {
       if (n <= 1 ){
            return 1;
        } 
    	Integer a = 0;
        Integer b = 1;
        Integer result = 0;
    	for(Integer i = 1; i<n; i++){
            result = a + b;
            a = b;
            b = result;}
        
      System.debug(result);
      return result;

}